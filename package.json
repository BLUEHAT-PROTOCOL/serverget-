{
  "name": "bot-cloud-controller",
  "version": "3.1.4",
  "description": "Advanced Bot Control Panel with Token Extraction and Auto-Deployment",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "node test-connection.js",
    "debug": "node --inspect server.js",
    "chrome-fix": "node chrome-fix.js",
    "setup": "npm install && mkdir -p uploads && echo 'Setup complete'",
    "reset": "rm -rf uploads tokens.db.json && mkdir uploads && echo 'Reset complete'",
    "backup": "tar -czf backup_$(date +%Y%m%d_%H%M%S).tar.gz uploads tokens.db.json server.js package.json"
  },
  "keywords": [
    "bot",
    "controller",
    "token",
    "extraction",
    "discord",
    "telegram",
    "hacker",
    "panel"
  ],
  "author": "OxyX System",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "fs-extra": "^11.2.0",
    "chokidar": "^3.5.3",
    "archiver": "^6.0.1",
    "extract-zip": "^2.0.1",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oxyx-system/bot-controller.git"
  },
  "bugs": {
    "url": "https://github.com/oxyx-system/bot-controller/issues"
  },
  "homepage": "https://github.com/oxyx-system/bot-controller#readme",
  "private": true,
  "os": [
    "linux",
    "windows",
    "darwin"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}